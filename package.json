{
    "name": "ngx",
    "displayName": "ngx",
    "description": "ngx",
    "version": "0.0.1",
    "publisher": "dadigua",
    "engines": {
        "vscode": "^1.15.0"
    },
    "categories": [
        "Languages"
    ],
    "extensionDependencies": [
        "html"
    ],
    "contributes": {
        "languages": [
            {
                "id": "html",
                "aliases": [
                    "ngx"
                ],
                "extensions": [
                    ".ngx"
                ]
            }
        ],
        "grammars": [
            {
                "language": "html",
                "scopeName": "source.ngx",
                "path": "./syntaxes/ngx.tmLanguage.json",
                "embeddedLanguages": {
                    "text.html": "html",
                    "source.css": "css",
                    "source.js": "javascript",
                    "source.python": "python",
                    "source.smarty": "smarty",
                    "source.ts": "typescript"
                  }
            }
        ]
    }
}